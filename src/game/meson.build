                        include_directories: game_inc,
                        dependencies: [zlib_dep, boost_dep, mariadb_dep, mysql_dep, postgresql_dep],
                        link_with: [frameworklib, libutf8cpp, sharedlib, g3dlib, detourlib],
                        cpp_args: game_args,
                        cpp_pch: pch_files,
                        install : false
)
endif
